<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fragment.ProfileFragment">


    <ImageButton
        android:id="@+id/star_button"
        android:layout_width="@dimen/star_botton_width"
        android:layout_height="@dimen/star_botton_height"
        android:layout_constraintBottom_toTopOf="@+id/update"
        android:layout_constraintHorizontal_bias="0.743"

        android:layout_constraintVertical_bias="0.949"
        android:layout_constraintEnd_toEndOf="parent"
        android:layout_constraintStart_toStartOf="parent"
        android:layout_constraintTop_toTopOf="parent"
        android:layout_marginStart="@dimen/star_botton_marginStar"
        android:layout_marginTop="@dimen/star_botton_marginTop"
        android:layout_marginEnd="@dimen/star_botton_marginEnd"
        android:layout_marginBottom="@dimen/star_botton_marginBottom"
        android:background="@color/alb"
        android:src="@drawable/ic_star_black_24dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.746"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.474" />

    <Button
        android:id="@+id/home"
        android:layout_width="@dimen/home_width"
        android:layout_height="@dimen/home_height"
        android:layout_constraintEnd_toStartOf="@+id/logout"
        android:layout_constraintHorizontal_bias="0.471"
        android:layout_constraintVertical_bias="0.95"
        android:layout_constraintBottom_toBottomOf="parent"
        android:layout_constraintStart_toStartOf="parent"
        android:layout_constraintTop_toTopOf="parent"
        android:layout_marginStart="@dimen/star_botton_marginStar"
        android:layout_marginTop="@dimen/star_botton_marginTop"
        android:layout_marginEnd="@dimen/star_botton_marginEnd"
        android:layout_marginBottom="@dimen/star_botton_marginBottom"
        android:background="@color/endblue"
        android:text="Home"
        android:textColor="@color/alb"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.285"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.935"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/logout"
        android:layout_width="@dimen/logout_width"
        android:layout_height="@dimen/logout_height"
        android:layout_constraintHorizontal_bias="0.86"
        android:layout_constraintVertical_bias="0.947"
        android:layout_constraintBottom_toBottomOf="parent"
        android:layout_constraintEnd_toEndOf="parent"
        android:layout_constraintStart_toStartOf="parent"
        android:layout_constraintTop_toTopOf="parent"
        android:layout_marginStart="@dimen/star_botton_marginStar"
        android:layout_marginTop="@dimen/star_botton_marginTop"
        android:layout_marginEnd="@dimen/star_botton_marginEnd"
        android:layout_marginBottom="@dimen/star_botton_marginBottom"
        android:background="@color/endblue"
        android:text="Logout"
        android:textColor="@color/alb"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.89"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.935"
        tools:ignore="MissingConstraints" />


    <TextView
        android:id="@+id/Treasures_discovered"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_constraintHorizontal_bias="0.118"
        android:layout_constraintVertical_bias="0.693"
        android:layout_constraintBottom_toBottomOf="parent"
        android:layout_constraintEnd_toEndOf="parent"
        android:layout_constraintStart_toStartOf="parent"
        android:layout_constraintTop_toTopOf="parent"
        android:layout_marginStart="@dimen/star_botton_marginStar"
        android:layout_marginTop="@dimen/star_botton_marginTop"
        android:layout_marginEnd="@dimen/star_botton_marginEnd"
        android:layout_marginBottom="@dimen/star_botton_marginBottom"
        android:autofillHints=""
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Treasures discovered"
        android:textColor="@color/Black"
        app:layout_constraintBottom_toTopOf="@+id/Treasures_hidden"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.171"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.975" />

    <TextView
        android:id="@+id/Username"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_constraintHorizontal_bias="0.118"
        android:layout_constraintVertical_bias="0.619"
        android:layout_constraintBottom_toBottomOf="parent"
        android:layout_constraintEnd_toEndOf="parent"
        android:layout_constraintStart_toStartOf="parent"
        android:layout_constraintTop_toTopOf="parent"
        android:layout_marginStart="@dimen/star_botton_marginStar"
        android:layout_marginTop="@dimen/star_botton_marginTop"
        android:layout_marginBottom="@dimen/star_botton_marginBottom"
        android:ems="10"
        android:text="Username"
        android:textColor="@color/Black"
        app:layout_constraintBottom_toTopOf="@+id/Treasures_discovered"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.165"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.962" />


    <TextView
        android:id="@+id/Treasures_hidden"
        :layout_constraintEnd_toEndOf="parent"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_constraintHorizontal_bias="0.12"
        android:layout_constraintVertical_bias="0.764"
        android:layout_constraintBottom_toBottomOf="parent"
        android:layout_constraintStart_toStartOf="parent"
        android:layout_constraintTop_toTopOf="parent"
        android:layout_marginStart="@dimen/star_botton_marginStar"
        android:layout_marginTop="@dimen/star_botton_marginTop"
        android:layout_marginEnd="@dimen/star_botton_marginEnd"
        android:layout_marginBottom="@dimen/star_botton_marginBottom"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Treasures hidden"
        android:textColor="@color/Black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.171"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.794" />

    <Button
        android:id="@+id/update"
        android:layout_width="@dimen/update_width"
        android:layout_height="@dimen/update_height"
        android:layout_constraintBottom_toBottomOf="parent"
        android:layout_constraintEnd_toEndOf="parent"
        android:layout_constraintStart_toStartOf="parent"
        android:layout_constraintTop_toTopOf="parent"
        android:layout_marginStart="@dimen/star_botton_marginStar"
        android:layout_marginTop="@dimen/star_botton_marginTop"
        android:layout_marginEnd="@dimen/star_botton_marginEnd"
        android:layout_marginBottom="@dimen/star_botton_marginBottom"
        android:background="@color/alb"
        android:text="Update"
        android:textColor="@color/endblue"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.586" />

    <ImageView
        android:id="@+id/profile_image_view"
        android:layout_width="@dimen/profile_image_view_width"
        android:layout_height="@dimen/profile_image_view_height"
        android:layout_constraintBottom_toTopOf="@+id/update"
        android:layout_constraintVertical_bias="0.212"
        android:layout_constraintEnd_toEndOf="parent"
        android:layout_constraintStart_toStartOf="parent"
        android:layout_constraintTop_toTopOf="parent"
        android:layout_marginStart="@dimen/star_botton_marginStar"
        android:layout_marginTop="@dimen/star_botton_marginTop"
        android:layout_marginEnd="@dimen/star_botton_marginEnd"
        android:layout_marginBottom="@dimen/star_botton_marginBottom"
        android:srcCompat="@android:drawable/alert_dark_frame"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.55"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.141" />

    <TextView
        android:id="@+id/score"
        android:layout_width="@dimen/score_width"
        android:layout_height="@dimen/score_height"
        android:layout_constraintBottom_toTopOf="@+id/update"
        android:layout_constraintEnd_toStartOf="@+id/star_button"
        android:layout_constraintHorizontal_bias="0.816"
        android:layout_constraintVertical_bias="0.895"
        android:layout_constraintTop_toBottomOf="@+id/profile_image_view"
        android:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="@dimen/star_botton_marginStar"
        android:layout_marginTop="@dimen/star_botton_marginTop"
        android:layout_marginEnd="@dimen/star_botton_marginEnd"
        android:layout_marginBottom="@dimen/star_botton_marginBottom"
        android:text="Score"
        android:textColor="@color/Black"
        app:layout_constraintBottom_toTopOf="@+id/update"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.33"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.935" />

</android.support.constraint.ConstraintLayout>